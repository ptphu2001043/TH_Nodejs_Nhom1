<h1>
    <%= data.title %>
</h1>

<table class="table">
    <tbody class="table-group-divider">
        <tr>
            <th scope="col">Username</th>
            <td>
                <%= data.row.username %>
            </td>
        </tr>
        <tr>
            <th scope="col">Họ tên</th>
            <td>
                <%= data.row.fullname %>
            </td>
        </tr>
        <tr>
            <th scope="col">Địa chỉ</th>
            <td>
                <%= data.row.address %>
            </td>
        </tr>
        <tr>
            <th scope="col">Giới tính</th>
            <td>
                <%= data.row.sex %>
            </td>
        </tr>
        <tr>
            <th scope="col">Email</th>
            <td>
                <%= data.row.email %>
            </td>
        </tr>
    </tbody>
</table>